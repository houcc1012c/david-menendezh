{
  "server": {
    "port": 7070,
    "url": "http://localhost:7070"
  },
  "endpoints": {
    "enabled": [
      "okn-mint",
      "domain",
      "mint",
      "export",
      "wings",
	  "dbpedia"
    ],
    "domain": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://w3id.org",
      "path": "/wings/export/",
      "graph": true
    },
    "okn-mint": {
      "endpointQuery": "https://endpoint.mint.isi.edu/modelCatalog-1.0.0/query",
      "base": "https://w3id.org",
      "path": "/okn/i/mint"
    },
    "mint": {
      "endpointQuery": "https://endpoint.mint.isi.edu/ds/query",
      "base": "https://w3id.org",
      "path": "/mint/instance"
    },
    "export": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://www.opmw.org",
      "path": "/export/",
      "graph": true
    },
    "wings": {
      "endpointQuery": "https://endpoint.mint.isi.edu/provenance/query",
      "base": "https://w3id.org",
      "path": "wings"
    },
	"dbpedia": {
		"endpointQuery": "https://dbpedia.org/sparql",
		"base": "http://dbpedia.org"
		}
  }
}
